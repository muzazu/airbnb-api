  "bugs": {
    "url": "https://github.com/muzazu/airbnb-api/issues"
  },
  "homepage": "https://github.com/muzazu/airbnb-api#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "db-migrate": "^0.11.1",
    "express": "^4.16.3",
    "mongodb": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.0.1",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2"
